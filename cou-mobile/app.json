{
  "expo": {
    "name": "COU",
    "slug": "COU",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/svg/COU_Logo.svg",
    "scheme": "cou-mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/svg/COU_Logo.svg",
        "backgroundColor": "#ffafa"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/svg/COU_Logo.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/icon.png",
            "./assets/svg/Bell.svg",
            "./assets/svg/Home.svg",
            "./assets/svg/TimeTable.svg"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/svg/COU_Logo.svg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFAFA"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
